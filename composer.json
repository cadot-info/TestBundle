{
  "name": "cadot.info/crudbundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "description": "Beautiful CRUD from entites from parameters for Symfony5",
  "keywords": [
    "symfony 5",
    "symfony",
    "crud"
  ],
  "require": {
    "php": ">=7.2",
    "symfony/framework-bundle": "^5.0",
    "symfony/templating": "^5.0"
  },
  "require-dev": {
    "twig/twig": "^3.0",
    "symfony/twig-bundle": "^5.0",
    "symfony/twig-bridge": "^5.0",
    "phpunit/phpunit": "^8.5",
    "symfony/browser-kit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "cadot.info\\CrudBundle\\": "src",
      "cadot.info\\CrudBundle\\Tests\\": "tests"
    }
  }
}
